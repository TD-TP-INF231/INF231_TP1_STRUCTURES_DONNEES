#include <stdio.h>

int produitAddition(int a, int b) {
    int result = 0;
    for (int i = 0; i < b; i++) {
        result += a;
    }
    return result;
}

int main() {
    int a = 3;
    int b = 4;

    int result = produitAddition(a, b);

    printf("%d Ã— %d = %d\n", a, b, result);

    return 0;
}