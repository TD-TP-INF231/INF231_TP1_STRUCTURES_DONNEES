#include <stdio.h>

int multiplication(int a, int b) {
    int result = 0;
    for (int i = 0; i < b; i++) // Répète b fois
        for (int j = 0; j < a; j++) // Ajoute 1, a fois
            result += 1;
    return result;
}

int main() {
    int a, b;
    printf("Entrez deux nombres positifs a et b : ");
    scanf("%d %d", &a, &b);
    
    if (a <= 0 || b <= 0) {
        printf("Les nombres doivent etre positifs.\n");
        return 1;
    }
    
    printf("%d x %d = %d\n", a, b, multiplication(a, b));
    return 0;
}