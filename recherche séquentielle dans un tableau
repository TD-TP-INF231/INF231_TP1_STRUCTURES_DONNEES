#include <stdio.h>

int rechercheSequentielle(int tab[], int n, int x) {
    for (int i = 0; i < n; i++) {
        if (tab[i] == x) {
            return i;
        }
    }
    return -1;
}

int main() {
    int tab[] = {1, 2, 3, 4, 5};
    int n = sizeof(tab) / sizeof(tab[0]);
    int x = 3;

    int result = rechercheSequentielle(tab, n, x);

    if (result != -1) {
        printf("Élément trouvé à l'indice %d\n", result);
    } else {
        printf("Élément non trouvé\n");
    }

    return 0;
}