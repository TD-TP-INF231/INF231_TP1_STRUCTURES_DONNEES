#include <stdio.h>

int rechercheSequentielle(int tab[], int n, int x) {
    for (int i = 0; i < n; i++) {
        if (tab[i] == x) {
            return i;
        }
    }
    return -1;
}

int main() {
    int n,x;
printf ("entrez la taille du tableau");
scanf("%d",&n);
printf("entrez l'élément rechercher:);
scanf("%d",&x);

    int result = rechercheSequentielle(tab, n, x);

    if (result != -1) {
        printf("Élément trouvé à l'indice %d\n", result);
    } else {
        printf("Élément non trouvé\n");
    }

    return 0;
}
